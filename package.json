{
  "name": "@harounabidi/zappicon",
  "version": "1.1.14",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    },
    "./icons/*": {
      "import": "./dist/esm/icons/*.js",
      "require": "./dist/cjs/icons/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "icons",
    "react",
    "svg",
    "ui",
    "components",
    "zappicon"
  ],
  "author": "Haroun Abidi",
  "license": "ISC",
  "description": "A comprehensive React icon library with multiple variants (filled, outline, duotone, light, regular)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harounabidi/zappicon-react.git"
  },
  "homepage": "https://github.com/harounabidi/zappicon-react#readme",
  "bugs": {
    "url": "https://github.com/harounabidi/zappicon-react/issues"
  },
  "scripts": {
    "prepare": "husky",
    "clean": "rm -rf dist && rm -rf src/icons",
    "generate": "npm run clean && node scripts/generate.mjs",
    "build": "npm run clean && npm run generate && rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run build && npm test"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.8",
    "@types/react": "^19.1.9",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jiti": "^2.5.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rollup": "^4.46.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-filesize": "^10.0.0",
    "svgo": "^4.0.0",
    "svgson": "^5.3.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.1-alpha.4"
  }
}
